number.py

#!/usr/bin/python3
import re

def main():
        pass
        ls = []
        fh = open('number.txt', 'r')
        for i in fh:
                #print(fh.read())
                sex = re.findall(r'^\+91\d{10}',i)
                print(sex)
if __name__ == '__main__':
        main()
