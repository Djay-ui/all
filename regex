regex.py

#!/usr/bin/python3

import re

def main():
        pass
        ls = []
        fh = open('stuti.txt', 'r')
        for i in fh:
                #print(fh.read())
                sex = re.findall(r'\w*\.\w*',i)
                print(sex)
if __name__ == '__main__':
        main()

